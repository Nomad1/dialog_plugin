{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/godot-cpp-3.x",
                "${workspaceFolder}/godot-cpp-3.x/include",
                "${workspaceFolder}/godot-cpp-3.x/include/core",
                "${workspaceFolder}/godot-cpp-3.x/include/gen",
                "${workspaceFolder}/godot-cpp-3.x/godot-headers"
            ],
            "defines": [
                "GDNATIVE"
            ],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64"
        }
    ],
    "version": 4
}