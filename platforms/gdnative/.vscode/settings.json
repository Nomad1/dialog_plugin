{
    "files.exclude": {
        "**/.*": true,
        "**/*.o" : true,
        "**/*.a" : true,
        "**/*.d" : true,
        "**/*.os" : true
    },
    "files.associations": {
        "*.shader": "glsl",
        "array": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "vector": "cpp",
        "__config": "cpp",
        "__nullptr": "cpp",
        "__bit_reference": "cpp",
        "__node_handle": "cpp",
        "atomic": "cpp",
        "bitset": "cpp",
        "__memory": "cpp",
        "limits": "cpp",
        "locale": "cpp",
        "optional": "cpp",
        "ratio": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "chrono": "cpp",
        "cstddef": "cpp",
        "sstream": "cpp",
        "typeinfo": "cpp"
    }
}